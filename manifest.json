{
"manifest_version": 2,
"name" :"Chrome Focus",
"version" :"1",
"description" :"An add on for Pompom",
"icons": { "128": "images/icon128-2.png" },
"browser_action" : {
  "default_icon": "images/icon128-1.png",
	"default_popup": "html/popup.html",
  "default_title": "Work Mode"
},
"background": {
  "scripts": ["js/background.js"],
  "persistent": false
},
"permissions": [
  "webNavigation",
  "tabs",
  "storage"
]
}
